# GitHub Actions Runner Configuration
# Copy this file to .env and fill in your values

# GitHub Personal Access Token with repo and admin:org permissions
GITHUB_TOKEN=your_github_token_here

# GitHub repository in format: owner/repository
GITHUB_REPOSITORY=your-username/your-repository

# GitHub organization or username
GITHUB_OWNER=your-username

# Number of runners to create (default: 1)
RUNNER_COUNT=1

# Optional: Customize runner names and labels
RUNNER_NAME=ubuntu-noble-runner
RUNNER_GROUP=default
RUNNER_LABELS=ubuntu,ubuntu-24.04,noble,self-hosted
